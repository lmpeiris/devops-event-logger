{
  "jira": {
    "issue_columns": ["Issue key", "Issue id", "Reporter Id", "Created", "Updated", "Resolved", "Parent", "Issue Type",
      "Reporter", "Project key"],
    "get_all_events_order": ["get_all_events"],
    "issue_source": {
      "type": "jira"
    },
    "preserve_timezone": false
  },
  "gitlab": {
    "get_all_events_order": ["get_issues_events", "get_branch_events", "get_mrs_events", "get_commit_events", "get_pipeline_events"],
    "preserve_timezone": false,
    "case_type_prefixes": {
                  "issue": "GLI",
                  "mr": "MR",
                  "pipeline": "GLPL",
                  "commit": "GLC",
                  "branch": "GLRF",
                  "release": "GLR",
                  "action_prefix": "gl"
    }
  },
  "azure_devops": {
    "get_all_events_order": ["get_issues_events", "get_branch_events", "get_mrs_events", "get_commit_events", "get_pipeline_events"],
    "preserve_timezone": false,
    "case_type_prefixes": {
                  "issue": "AZDI",
                  "mr": "AZDMR",
                  "pipeline": "AZDPL",
                  "commit": "AZDC",
                  "branch": "AZDRF",
                  "release": "AZDR",
                  "action_prefix": "azd"
    }
  }
}